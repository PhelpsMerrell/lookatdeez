{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.js", "/*.css", "/*.ico", "/*.png"]
  },
  "mimeTypes": {
    ".dart": "text/plain"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
